{% extends 'SonataAdminBundle::standard_layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/applicationsonatadevises/css/devises.css') }}" type="text/css"  media="all"/>
{% endblock stylesheets %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">
        var uniqid = '{{ admin.uniqid }}';
        {% if js_settings_json is defined and js_settings_json is not empty %}
        var Sonata = {};
        jQuery.extend(Sonata, {{ js_settings_json|raw }});
        {% endif%}
    </script>

    <script src="{{ asset('bundles/applicationsonatadevises/js/devises.js') }}" type="text/javascript"></script>
{% endblock javascripts %}
