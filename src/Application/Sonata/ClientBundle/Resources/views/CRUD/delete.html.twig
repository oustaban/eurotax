{% extends 'SonataAdminBundle:CRUD:delete.html.twig' %}

{% block title %}
{% if client is defined %}
{% set label = client ~ ' - ' ~ parent() %}
{% elseif  current_client is defined and current_client is not empty %}
{% set label = 'Client - '  ~ current_client.getCodeClient ~  ' - ' ~ current_client  %}
{% else %}
{% set label = 'Client - Delete' %}
{% endif %}
{{ label }}

{% endblock title %}


