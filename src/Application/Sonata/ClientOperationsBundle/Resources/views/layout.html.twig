{% extends 'SonataAdminBundle::standard_layout.html.twig' %}

{% block formTabs %} {% endblock formTabs %}
{% block popup_form %} {% endblock popup_form %}

{% block sonata_admin_content %}

{% block panel_block %}{% endblock panel_block %}
{{ block('formTabs') }}

{% if content is not empty %}
<div class="sonata-ba-form sonata-ba-list">
    {{ content|raw }}
    {{ block('popup_form') }}
</div>
{% endif %}

{% endblock sonata_admin_content %}

{% block title %}  {% endblock title %}
{% block breadcrumb %} {% endblock breadcrumb %}

{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" href="{{ asset('bundles/applicationsonataclientoperations/css/client_operation.css') }}" type="text/css" media="all"/>
{% endblock stylesheets %}

{% block javascripts %}
{{ parent() }}
<script src="{{ asset('bundles/applicationsonataclientoperations/js/client_operation.js') }}" type="text/javascript"></script>
{% endblock javascripts %}
