{% block ajaxCreate_js %}{% endblock ajaxCreate_js %}

{% block ajaxCreate %}
{% include 'ApplicationSonataClientOperationsBundle:JS:list_layout.js.twig'%}
<script type="text/javascript">
    $(function () {
        $('.sonata-actions .sonata-action-element a').live('click', function (event) {
            var link = $(this).attr('href');
            field_dialog_form_add_{{ id }}(event, link);
            return false;
        });
    });
</script>
{% endblock ajaxCreate %}