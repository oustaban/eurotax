
<table cellpadding="0" cellspacing="0" border="0">
    <thead>
    <tr>
    	<td>&nbsp;</td>
    	<td>&nbsp;</td>
    	<td class="th_nett o_nett">{{ "ApplicationSonataClientOperationsBundle.declaration.th.nett"|trans() }}</td>
    	<td class="th_tva o_tva">{{ "ApplicationSonataClientOperationsBundle.declaration.th.tva"|trans() }}</td>
    </tr>
    <tr>
        <th colspan="4">{{ "ApplicationSonataClientOperationsBundle.declaration.vat_purchases"|trans() }} ({{ "ApplicationSonataClientOperationsBundle.declaration.period_prev_month"|trans() }})</th>
    </tr>
    </thead>
    <tbody>
    
    {% for data in A02TVAPrevlist %}
    <tr>
        <td class="i_title">{% if loop.index == 1 %} Purchases {% endif %}</td>
        <td class="i_value">{{ (data.getTauxDeTVA() * 100) }}%</td>
        <td class="i_nett">&nbsp;</td>
        <td class="i_tva">{{ data.getTVA()|number_format(0, ',', ' ') }}</td>
    </tr>
    {% endfor %}
    
    {% for data in A08IMPrevlist %}
    <tr>
        <td class="i_title">{% if loop.index == 1 %} Importation {% endif %}</td>
        <td class="i_value">{{ (data.getTauxDeTVA() * 100) }}%</td>
        <td class="i_nett">&nbsp;</td>
        <td class="i_tva">{{ data.getTVA()|number_format(0, ',', ' ') }}</td>
    </tr>
    {% endfor %}
    
    </tbody>
</table>
