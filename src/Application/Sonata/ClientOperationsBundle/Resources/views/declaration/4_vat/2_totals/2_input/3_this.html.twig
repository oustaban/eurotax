<table cellpadding="0" cellspacing="0" border="0">
    <thead>
    
    <tr>
    	<td>&nbsp;</td>
    	<td>&nbsp;</td>
    	<td class="th_nett o_nett">{{ "ApplicationSonataClientOperationsBundle.declaration.th.nett"|trans() }}</td>
    	<td class="th_tva o_tva">{{ "ApplicationSonataClientOperationsBundle.declaration.th.tva"|trans() }}</td>
    </tr>
    <tr>
        <th colspan="4">{{ "ApplicationSonataClientOperationsBundle.declaration.vat_purchases"|trans() }} ({{ "ApplicationSonataClientOperationsBundle.declaration.period_this_month"|trans() }})</th>
    </tr>
    </thead>
    <tbody>
   
    
    {% for data in A02TVAlist %}
    <tr>
        <td class="i_title">{% if loop.index == 1 %}  {{ "ApplicationSonataClientOperationsBundle.declaration.purchases"|trans() }}{% endif %}</td>
        <td class="i_value">{{ (data.getTauxDeTVA() * 100) }}%</td>
        <td class="i_nett">&nbsp;</td>
        <td class="i_tva">{{ data.getTVA()|number_format(0, ',', ' ') }}</td>
    </tr>
    {% endfor %}
    
    
    
    {% for data in A08IMlist %}
    <tr>
        <td class="i_title">{% if loop.index == 1 %}  {{ "ApplicationSonataClientOperationsBundle.declaration.importation"|trans() }}{% endif %}</td>
        <td class="i_value">{{ (data.getTauxDeTVA() * 100) }}%</td>
        <td class="i_nett">&nbsp;</td>
        <td class="i_tva">{{ data.getTVA()|number_format(0, ',', ' ') }}</td>
    </tr>
    {% endfor %}
    
    
   
    </tbody>
</table>
