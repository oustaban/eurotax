<table cellpadding="0" cellspacing="0" border="0">
    <thead>
    
    <tr>
    	<td>&nbsp;</td>
    	<td>&nbsp;</td>
    	<td class="th_nett o_nett">{{ "ApplicationSonataClientOperationsBundle.declaration.th.nett"|trans() }}</td>
    	<td class="th_tva o_tva">{{ "ApplicationSonataClientOperationsBundle.declaration.th.tva"|trans() }}</td>
    </tr>
    
    <tr>
        <th colspan="4">{{ "ApplicationSonataClientOperationsBundle.declaration.sales"|trans() }}</th>
    </tr>
    </thead>
    <tbody>
    
    {% for data in V01TVAlist %}
    <tr>
        <td class="o_title">{% if loop.index == 1 %} <!--  {{ "ApplicationSonataClientOperationsBundle.exports.V01TVA.title"|trans() }}--> {% endif %}</td>
        <td class="o_value">{{ (data.getTauxDeTVA() * 100) }}%</td>
        <td class="o_nett">{{ data.getHT()|number_format(0, ',', ' ') }}</td>
        <td class="o_tva">{{ data.getTVA()|number_format(0, ',', ' ') }}</td>
    </tr>
    {% endfor %}

    </tbody>
</table>