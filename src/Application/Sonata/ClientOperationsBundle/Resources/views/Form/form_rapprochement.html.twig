<form action="?filter%5Bclient_id%5D%5Bvalue%5D={{ client.id }}&month={{ info.month }}|{{ info.year }}" method="post" {{ form_enctype(form) }} id="rapprochement_form" class="">


	<a href="#" id="intro_info_id_hidden" value="{{ intro_info_id }}"></a>
	<a href="#" id="exped_info_id_hidden" value="{{ exped_info_id }}"></a>


	<div class="alert-error">
 	{{ form_errors(form.intro_info_id) }}
 	{{ form_errors(form.intro_info_text) }}
 	{{ form_errors(form.exped_info_id) }}
 	{{ form_errors(form.exped_info_text) }}
	{{ form_errors(form) }}
	</div>

	<div class="rapprochement_content_deb">
            <div class="rapprochement_content_input rapprochement_intro">
                <div class="rapprochement_content_body_table">
				<table class="table table-bordered table-striped">
	                <tbody>
	                <tr>
	                    <td width="40%">
	                    {{ form_widget(form.intro_info_id) }}
	                    <p>{{ form_widget(form.intro_info_text) }}</p>
	                    </td>
	                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
	                    	<p>{{ form_widget(form.intro_info_number) }}{{ form_errors(form.intro_info_number) }}</p>
	                    </td>
	                     <td width="20%">&nbsp;</td>
	                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
							<p>{{ form_widget(form.intro_info_number2) }}{{ form_errors(form.intro_info_number2) }}</p>
	                    </td>
	                </tr>
	                </tbody>
	            </table>
				</div>
            </div>
            <div class="rapprochement_content_output rapprochement_exped">
                <div class="rapprochement_content_body_table">
				<table class="table table-bordered table-striped">
	                <tbody>
	                <tr>
	                    <td width="40%">
	                    {{ form_widget(form.exped_info_id) }}
	                    <p>{{ form_widget(form.exped_info_text) }}</p>
	                    </td>
	                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
	                    	<p>{{ form_widget(form.exped_info_number) }}{{ form_errors(form.exped_info_number) }}</p>                    
	                    </td>
	                    <td width="20%">&nbsp;</td>
	                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
							<p>{{ form_widget(form.exped_info_number2) }}{{ form_errors(form.exped_info_number2) }}</p>
							
	                    </td>
	                </tr>
	                </tbody>
	            </table>
				</div>
            </div>
        </div>
        {{ form_rest(form) }}

<div class="rapprochement_content_stat">
        <div class="rapprochement_content_input">
            <table class="table table-bordered table-striped">
                <tbody>
                <tr>
                    <td width="40%">
                        ECART 3
                    </td>
                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
                        <div id="totals_input_calcu1"></div>
                    </td>
                    <td width="20%">
                        &nbsp;
                    </td>
                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
                        <div id="totals_input_calcu2"></div>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="rapprochement_content_output">
            <table class="table table-bordered table-striped">
                <tbody>
                <tr>
                    <td width="40%">
                        ECART 4
                    </td>
                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
                        <div id="totals_output_calcu1"></div>
                    </td>
                    <td width="20%">
                        &nbsp;
                    </td>
                    <td class="sonata-ba-list-field sonata-ba-list-field-money" width="20%">
                        <div id="totals_output_calcu2"></div>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>






<div class="content cboth ">
	&nbsp;&nbsp;
	{% if info.blocked %}
	    {% set text_lock = info.blocked ? 'Clôturer' : 'Déverrouiller' %}
	    {% if fromImport == false %}
	    	<button id="btn_locking" name="btn_locking" rel="tooltip" title="{{ text_lock|trans}}" class="btn pull-right"><i class="icon icon-lock"></i> {{ text_lock|trans}}</button>
	    {% endif %}
    {% endif %}
    
    <!-- <button id="btn_saving" name="btn_saving" rel="tooltip" title="Save" class="btn pull-right"><i class="icon icon-ok-sign"></i> Save</button> -->
    
    <a href="{{ listLink }}" id=""  rel="tooltip" title="Annuler" class="btn pull-right"><i class="icon icon-chevron-left"></i> Annuler</a>
        
    {% if hasImportDataOnly %}
    <a href="{{ declarationLink }}" target="_blank" class="btn pull-right"><i class="icon icon-print"></i> Declaration</a>
    {% else %}
    <a href="{{ listLink }}" class="btn pull-right"><i class="icon icon-list-alt"></i> Analyse des données saisies</a>
    {% endif %}
    

    
    
    
    <!--button id="btn_recalculer" rel="tooltip" title="Recalculer" class="btn pull-right"><i class="icon icon-lock"></i> Recalculer</button-->
    
</div>        
</form>